{
    "name": "oscar/oscar",
    "type": "project",
    "description": "The Oscar Car rental",
    "keywords": ["Oscar", "Car", "rental"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "fakerphp/faker": "1.20.*",
        "mikey179/vfsstream": "^1.6"
    },
    "require-dev": {
        "phpmd/phpmd": "@stable",
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "3.5.*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
